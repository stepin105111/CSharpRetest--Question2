@model MVCFinal.Models.ETable

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>NewEmployee</title>
</head>
<body>
    <h1>New Employee Registration</h1>
    <hr />
    <div>
        @using (Html.BeginForm())
        {
            <p>
                Enter the EmpID: @Html.EditorFor((m) => m.ID)
            </p>
            <p>
                Enter the EmpName: @Html.EditorFor((m) => m.Name)
            </p>
            <p>
                Enter the EmpAddress: @Html.EditorFor((m) => m.Address)
            </p>
            <p>
                Enter the EmpSalary: @Html.EditorFor((m) => m.Salary)
            </p>
            <button>Add Employee</button>
        }
    </div>
</body>
</html>

